<div class="jumbotron">
    <h1>ASP.NET</h1>
    <p class="lead">ASP.NET is a free web framework for building great Web sites and Web applications using HTML, CSS, and JavaScript.</p>
    <p><a href="https://asp.net" class="btn btn-primary btn-lg">Learn more &raquo;</a></p>
</div>
<form id="myForm">

    <input type="text" placeholder="from" name="from" id="from1">




    <input type="text" placeholder="to" name="to" id="to1">

    <input id="sendBtn" type="submit" value="input Date Interval" />
   

</form>
@section scripts{
    <script>
        $(function () {
            $("#myForm").submit(function (e) {
                e.preventDefault();
                let a = $("#to1").val();
                let b = $("#from1").val();
                let myUrl = "";
                myUrl += ("\\api\\values\\" + a + "\\" + b);
                $.ajax({
                    url: myUrl,
                    method: "post",
                    success: function (data) {
                        console.log(data);
                    }
                })
            })
        })
    </script>
   
}